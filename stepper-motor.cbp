<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="stepper-motor" />
		<Option pch_mode="2" />
		<Option compiler="avrgcc" />
		<Build>
			<Target title="Debug">
				<Option type="4" />
				<Option compiler="avrgcc" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-g" />
				</Compiler>
				<ExtraCommands>
					<Add after="avr-size $(TARGET_OUTPUT_FILE)" />
					<Add after="avr-objcopy -O ihex -R .eeprom -R .eesafe $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).hex" />
					<Add after="avr-objcopy --no-change-warnings -j .eeprom --change-section-lma .eeprom=0 -O ihex $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).eep.hex" />
				</ExtraCommands>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/stepper-motor" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="avrgcc" />
				<Compiler>
					<Add option="-Os" />
				</Compiler>
				<ExtraCommands>
					<Add after="avr-size $(TARGET_OUTPUT_FILE)" />
					<Add after="avr-objcopy -O ihex -R .eeprom -R .eesafe $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).hex" />
					<Add after="avr-objcopy --no-change-warnings -j .eeprom --change-section-lma .eeprom=0 -O ihex $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).eep.hex" />
					<Add after="avrdude -p t85 -c avrjake -P /dev/parport0 -U $(TARGET_OUTPUT_FILE).hex -q -q -s" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-mmcu=attiny85" />
			<Add option="-Wall" />
			<Add option="--std=gnu99" />
			<Add option="-DF_CPU=8000000UL" />
		</Compiler>
		<Linker>
			<Add option="-mmcu=attiny85" />
		</Linker>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<envvars />
			<code_completion>
				<search_path add="/usr/lib/avr/include" />
			</code_completion>
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
